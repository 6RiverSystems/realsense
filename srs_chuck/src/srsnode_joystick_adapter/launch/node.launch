<launch>
    <node name="joy" pkg="joy" type="joy_node" required="true" />

    <remap from="/internal/sensors/joystick/velocity" to="/internal/drivers/brainstem/cmd_velocity"/>

    <node name="srsnode_joystick_adapter"
